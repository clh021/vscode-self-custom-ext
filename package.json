{
  "name": "lianghong-custom",
  "displayName": "良宏自用插件",
  "description": "lianghong self custom ext for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lianghongCustom.helloWorld",
    "onCommand:lianghongCustom.laozi",
    "onCommand:lianghongCustom.heroHelp",
    "onCommand:lianghongCustom.helpBody",
    "onCommand:lianghongCustom.getTea"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lianghongCustom.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "lianghongCustom.laozi",
        "title": "laozi random notify"
      },
      {
        "command": "lianghongCustom.heroHelp",
        "title": "hero random notify"
      },
      {
        "command": "lianghongCustom.helpBody",
        "title": "help body active"
      },
      {
        "command": "lianghongCustom.getTea",
        "title": "get a Tea notify"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
